{
    "name": "ygorg-frontend-sk",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write .",
        "lint": "prettier --check . && eslint .",
        "test:unit": "vitest",
        "test": "npm run test:unit -- --run && npm run test:e2e",
        "test:e2e": "playwright test",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "graphql:codegen": "houdini pull-schema",
        "ui:add": "pnpm dlx shadcn-svelte@next add"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^1.9.0",
        "@playwright/test": "^1.45.3",
        "@storybook/addon-essentials": "^8.3.6",
        "@storybook/addon-interactions": "^8.3.6",
        "@storybook/addon-links": "^8.3.6",
        "@storybook/addon-svelte-csf": "^5.0.0-next.10",
        "@storybook/blocks": "^8.3.6",
        "@storybook/svelte": "^8.3.6",
        "@storybook/sveltekit": "^8.3.6",
        "@storybook/test": "^8.3.6",
        "@sveltejs/adapter-auto": "^3.0.0",
        "@sveltejs/enhanced-img": "^0.3.10",
        "@sveltejs/kit": "^2.0.0",
        "@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
        "@tailwindcss/aspect-ratio": "^0.4.2",
