{
    "name": "ygorg-frontend-sk",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "packageManager": "pnpm@9.12.3",
    "engines": {
        "node": "^20",
        "pnpm": "^9.12.3"
    },
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write .",
        "lint": "prettier --check . && eslint .",
        "test:unit": "vitest",
        "test": "npm run test:unit -- --run && npm run test:e2e",
        "test:e2e": "playwright test",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "graphql:codegen": "houdini pull-schema",
        "ui:add": "pnpm dlx shadcn-svelte@next add"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^3.2.2",
        "@playwright/test": "^1.48.2",
        "@storybook/addon-essentials": "^8.4.1",
        "@storybook/addon-interactions": "^8.4.1",
        "@storybook/addon-links": "^8.4.1",
        "@storybook/addon-svelte-csf": "^5.0.0-next.10",
        "@storybook/blocks": "^8.4.1",
        "@storybook/svelte": "^8.4.1",
        "@storybook/sveltekit": "^8.4.1",
        "@storybook/test": "^8.4.1",
        "@sveltejs/adapter-auto": "^3.3.1",
        "@sveltejs/enhanced-img": "^0.4.0",
        "@sveltejs/kit": "^2.7.4",
        "@sveltejs/vite-plugin-svelte": "^4.0.0",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/container-queries": "^0.1.1",
        "@tailwindcss/forms": "^0.5.9",
        "@tailwindcss/typography": "^0.5.15",
        "@types/eslint": "^9.6.1",
        "autoprefixer": "^10.4.20",
        "bits-ui": "1.0.0-next.36",
        "clsx": "^2.1.1",
        "embla-carousel-svelte": "^8.3.1",
        "eslint": "^9.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.46.0",
        "globals": "^15.11.0",
        "houdini": "^1.3.0",
        "houdini-plugin-svelte-global-stores": "^1.3.0",
        "houdini-svelte": "^2.0.0",
        "lucide-svelte": "^0.454.0",
        "prettier": "^3.3.3",
        "prettier-plugin-svelte": "^3.2.7",
        "prettier-plugin-tailwindcss": "^0.6.8",
        "storybook": "^8.4.1",
        "svelte": "^5.1.9",
        "svelte-check": "^4.0.5",
        "tailwind-merge": "^2.5.4",
        "tailwind-variants": "^0.2.1",
        "tailwindcss": "^3.4.14",
        "tailwindcss-animate": "^1.0.7",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.12.2",
        "unplugin-icons": "^0.20.0",
        "vite": "^5.4.10",
        "vite-plugin-mkcert": "^1.17.6",
        "vite-plugin-tailwind-purgecss": "^0.3.3",
        "vitest": "^2.1.4"
    },
    "dependencies": {
        "@inlang/paraglide-sveltekit": "^0.11.5"
    },
    "overrides": {
        "@sveltejs/vite-plugin-svelte": "^4.0.0"
