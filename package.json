{
    "name": "ygorg-frontend-sk",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "packageManager": "pnpm@10.12.1",
    "engines": {
        "node": "^20",
        "pnpm": "^10.12.1"
    },
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write .",
        "lint": "prettier --check . && eslint .",
        "test:unit": "vitest",
        "test": "npm run test:unit -- --run && npm run test:e2e",
        "test:e2e": "playwright test",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "graphql:codegen": "houdini pull-schema",
        "dev:pages": "pnpm build && wrangler pages dev ./.svelte-kit/cloudflare"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^3.2.6",
        "@iconify-json/mdi": "^1.2.3",
        "@inlang/paraglide-js": "1.11.8",
        "@playwright/test": "^1.52.0",
        "@skeletonlabs/skeleton": "3.1.3",
        "@skeletonlabs/skeleton-svelte": "1.2.3",
        "@storybook/addon-essentials": "^8.6.14",
        "@storybook/addon-interactions": "^8.6.14",
        "@storybook/addon-links": "^8.6.14",
        "@storybook/addon-svelte-csf": "5.0.3",
        "@storybook/blocks": "^8.6.14",
        "@storybook/svelte": "^8.6.14",
        "@storybook/sveltekit": "^8.6.14",
        "@storybook/test": "^8.6.14",
        "@sveltejs/adapter-auto": "^4.0.0",
        "@sveltejs/adapter-cloudflare": "^5.1.0",
        "@sveltejs/enhanced-img": "^0.6.0",
        "@sveltejs/kit": "^2.21.3",
        "@sveltejs/vite-plugin-svelte": "^5.1.0",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/container-queries": "^0.1.1",
        "@tailwindcss/forms": "^0.5.10",
        "@tailwindcss/typography": "^0.5.16",
        "@types/eslint": "^9.6.1",
        "@types/node": "^22.10.1",
        "autoprefixer": "^10.4.21",
        "eslint": "^9.28.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-svelte": "^2.46.1",
        "globals": "^15.15.0",
        "houdini": "^1.5.6",
        "houdini-plugin-svelte-global-stores": "^1.4.12",
        "houdini-svelte": "^2.1.16",
        "postcss": "^8.5.4",
        "prettier": "^3.5.3",
        "prettier-plugin-svelte": "^3.4.0",
        "prettier-plugin-tailwindcss": "^0.6.12",
        "runed": "^0.28.0",
        "storybook": "^8.6.14",
        "svelte": "^5.33.18",
        "svelte-check": "^4.2.1",
        "tailwind-merge": "^3.3.0",
        "tailwind-variants": "^0.3.1",
        "tailwindcss": "^3.4.17",
        "tailwindcss-animate": "^1.0.7",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.33.1",
        "unplugin-icons": "^22.1.0",
        "vite": "^6.2.4",
        "vite-plugin-mkcert": "^1.17.8",
        "vite-plugin-tailwind-purgecss": "^0.3.5",
        "vitest": "^3.2.3"
    },
    "peerDependencyRules": {
        "allowedVersions": {
            "svelte": "5"
        }
    },
    "dependencies": {
        "@inlang/paraglide-sveltekit": "0.16.1",
        "svelte-persisted-state": "^1.2.0"
    },
    "overrides": {
        "@sveltejs/vite-plugin-svelte": "^5.1.0"
    }
}
